package stack;
package ;

import java.util.Scanner;

public class Main4 {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter Stack's size: ");
        Stack st = new Stack(sc.nextInt());
        while(!st.isFull()) {
            System.out.print("Insert element: ");
            st.addElement(sc.nextInt());
        }
    }
}
